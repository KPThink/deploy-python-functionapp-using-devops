                "scriptPath": "",
                "inlineScript": "$env=$env:dev\n$ResourceGroup=$env:ResourceGroup\n$Location=$env:Location\n$srcFolder=$env:srcFolder\n$FunName=$env:FunName\n\n\naz functionapp deployment source config-zip --resource-group $ResourceGroup --name $FunName --src $srcFolder\n",
